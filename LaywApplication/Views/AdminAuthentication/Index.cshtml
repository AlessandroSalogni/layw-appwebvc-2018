@model List<DoctorAssociatedPatients>

@{
    Layout = "Layouts/_Admin";
}

@foreach (DoctorAssociatedPatients associated in Model)
{
    <strong>@associated.Doctor.Name</strong><br />

    @foreach (Patient patient in associated.Doctor.Patients)
    {
        <i>@patient.Name</i>
    }

    <br /><br /><br />
}

